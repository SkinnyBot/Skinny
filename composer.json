{
    "name": "xety/skinny",
    "description": "A simple bot in PHP.",
    "homepage": "https://github.com/Xety/Skinny",
    "keywords": ["discord", "bot", "php", "discordphp", "botphp"],
    "license": "MIT",
    "support": {
        "source":"https://github.com/Xety/Skinny",
        "issues":"https://github.com/Xety/Skinny/issues"
    },
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": ">=5.6",
        "team-reflex/discord-php": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Skinny\\": "src"
        },
        "files": [
            "src/basics.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
