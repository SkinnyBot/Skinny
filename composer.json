{
    "name": "skinnybot/skinny",
    "description": "A framework to create discord bot in PHP.",
    "homepage": "https://github.com/SkinnyBot/Skinny",
    "keywords": ["discord", "bot", "php", "discordphp", "botphp", "skinny"],
    "license": "MIT",
    "support": {
        "source":"https://github.com/SkinnyBot/Skinny",
        "issues":"https://github.com/SkinnyBot/Skinny/issues"
    },
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": ">=7.3",
        "skinnybot/plugin-installer": "~2.0",
        "charlottedunois/yasmin": "^0.6.2",
        "envms/fluentpdo": "^2.1.0",
        "cakephp/chronos": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Skinny\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SkinnyTest\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
